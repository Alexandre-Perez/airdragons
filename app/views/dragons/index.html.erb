    <div class="wrapper">
     <% @dragons.each do |dragon| %>
        <div class="card-trip">
          <img src="https://www.dinosoria.com/dragon-1/images/dragon-5_jpg.jpg"/>
          <div class="card-trip-infos">
            <div>
              <p><%= dragon.name %></p>
              <p><%= dragon.address %></p>
            </div>
            <a href="#"><i class="fa-brands fa-d-and-d"></i></a>
            <h2 class="card-trip-pricing"><%= rand(500..3000) %>$</h2>

          </div>
        </div>
     <% end %>
    </div>
